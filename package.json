{
  "author": "Philippe ENTZMANN",
  "dependencies": {
    "d3-selection": "^1.3.0",
    "d3-zoom": "^1.7.1",
    "viz.js": "^2.0.0",
    "worker-loader": "^2.0.0"
  },
  "description": "A JupyterLab extension for viewing graphviz files.",
  "devDependencies": {
    "@jupyterlab/application": "^0.17.2",
    "@jupyterlab/rendermime-interfaces": "^1.1.2",
    "@phosphor/widgets": "^1.6.0",
    "@types/d3-selection": "^1.3.0",
    "@types/d3-zoom": "^1.7.1",
    "@types/webpack-env": "~1.13.6",
    "codemirror": "~5.39.0",
    "prettier": "1.13.7",
    "rimraf": "~2.6.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~2.9.2"
  },
  "files": [
    "{lib,style}/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "LICENSE"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "keywords": [
    "dot",
    "graphviz",
    "jupyter",
    "jupyterlab-extension",
    "jupyterlab",
    "neato"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "jupyterlab_graphviz",
  "peerDependencies": {
    "@jupyterlab/application": "^0.17.2",
    "@jupyterlab/rendermime-interfaces": "^1.1.2",
    "@phosphor/widgets": "^1.6.0",
    "codemirror": "~5.39.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PhE/jupyterlab_graphviz.git"
  },
  "scripts": {
    "bootstrap": "jlpm && jlpm clean && jlpm lint && jlpm build && jlpm lab:link && jlpm lab:build:dev",
    "build": "tsc --project src",
    "clean": "rimraf lib",
    "lab:build": "jupyter lab build",
    "lab:build:dev": "jupyter lab build --dev",
    "lab:link": "jupyter labextension link . --no-build",
    "lab:watch": "jupyter lab --watch",
    "lint": "tslint --project src --fix && prettier --write \"{src,style,typings}/**/*.{ts,css,json}\"",
    "watch": "jlpm build --watch"
  },
  "version": "0.3.0"
}
